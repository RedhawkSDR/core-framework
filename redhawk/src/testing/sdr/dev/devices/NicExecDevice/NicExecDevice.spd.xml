<softpkg id="DCE:b6c0cb8f-6a1e-4dc5-bfde-08a7d699c744" name="NicExecDevice" type="2.0.9">
  <title></title>
  <author>
    <name></name>
  </author>
  <description>Executable device with configurable virtual NICs to support testing components with NIC allocations.</description>
  <propertyfile type="PRF">
    <localfile name="NicExecDevice.prf.xml"/>
  </propertyfile>
  <descriptor>
    <localfile name="NicExecDevice.scd.xml"/>
  </descriptor>
  <implementation id="python">
